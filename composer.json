{
    "name": "joshuaestes/biga-framework-bundle",
    "type": "symfony-bundle",
    "description": "BigaFrameworkBundle for Symfony Biga Edition",
    "keywords": ["biga","symfony2","framework"],
    "homepage": "https://github.com/JoshuaEstes/biga-framework-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    }
    "authors": [
        {
            "name": "Joshua Estes",
            "email": "Joshua.Estes@ScenicCityLabs.com",
            "homepage": "http://joshuaestes.me",
            "role": "Lead Developer"
        }
    ],
    "support": {
        "email": "JoshuaEstes+BigaFrameworkBundle@ScenicCityLabs.com",
        "issues": "https://github.com/JoshuaEstes/biga-framework-bundle/issues",
        "wiki": "https://github.com/JoshuaEstes/biga-framework-bundle/wiki",
        "source": "https://github.com/JoshuaEstes/biga-framework-bundle"
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "2.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "dev-master",
        "symfony/console": "2.2.*",
        "symfony/locale": "2.2.*",
        "symfony/process": "2.2.*",
        "sensio/generator-bundle": "dev-master",
        "mikey179/vfsStream": "dev-master"
    },
    "autoload": {
        "psr-0": { "BigaFrameworkBundle": "" }
